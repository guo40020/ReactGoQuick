{
	"name": "react-ext",
	"displayName": "Kelly's React Extension",
	"description": "",
	"publisher": "Kelly",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:react-ext.CreateComponent",
		"onCommand:react-ext.CreateContext"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [{
			"command": "react-ext.CreateComponent",
			"title": "CreateComponent"
		},
		{
			"command": "react-ext.CreateContext",
			"title": "CreateContext"
		}],
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/snippets.json"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}